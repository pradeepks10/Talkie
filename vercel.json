{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
    
  }
  